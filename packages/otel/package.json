{
  "name": "@contextcompany/otel",
  "version": "1.0.11",
  "description": "The Context Company OpenTelemetry integration",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./nextjs": {
      "types": "./dist/nextjs/index.d.ts",
      "import": "./dist/nextjs/index.js",
      "require": "./dist/nextjs/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "pnpm build --watch",
    "serve": "npx serve dist -p 3002 --cors",
    "dev:all": "concurrently \"pnpm build --watch\" \"pnpm serve\""
  },
  "tsup": {
    "bundle": true,
    "external": [
      "@opentelemetry/api",
      "@vercel/otel"
    ]
  },
  "sideEffects": false,
  "keywords": [
    "opentelemetry",
    "observability",
    "tracing",
    "monitoring",
    "nextjs",
    "context-company"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/The-Context-Company/observatory",
    "directory": "packages/otel"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@vercel/otel": "^2.0.1"
  },
  "peerDependenciesMeta": {
    "@opentelemetry/api": {
      "optional": false
    },
    "@vercel/otel": {
      "optional": true
    }
  },
  "dependencies": {
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "ws": "^8.18.3",
    "posthog-node": "^5.11.2"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.2.0",
    "@opentelemetry/otlp-exporter-base": "^0.207.0",
    "@opentelemetry/otlp-transformer": "^0.205.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@types/node": "^20",
    "@types/ws": "^8.18.1",
    "@vercel/otel": "^2.0.1",
    "concurrently": "^9.2.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
